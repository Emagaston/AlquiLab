<?xml version="1.0" encoding="utf-8"?><!-- Adding the same root's ID for view binding as other layout configurations -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/alquileroferta_list_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/alquileroferta_list"
        android:name="com.example.alquilab.AlquilerOfertaListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="-2dp"
        app:layoutManager="LinearLayoutManager"
        tools:context="com.example.alquilab.AlquilerOfertaDetailHostActivity"
        tools:layout_editor_absoluteX="0dp"
        tools:listitem="@layout/alquileroferta_list_content" />

</RelativeLayout>