<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".DetalleAlquiler">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"/>

    <TextView
        android:id="@+id/detail_nom"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="169dp" />

    <TextView
        android:id="@+id/detail_des"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:scrollbars="vertical"
        android:layout_below="@+id/detail_nom"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="169dp" />

    <TextView
        android:id="@+id/detail_dir"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detail_des"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/detail_bar"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detail_dir"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/detail_hab"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detail_bar"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/detail_pre"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detail_hab"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="257dp" />

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_below="@+id/detail_pre"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp">
    </fragment>

    <TextView
        android:id="@+id/detail_imagen1"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/map"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:text="ImÃ¡genes"
        android:minHeight="48dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="257dp" />

    <ImageView
        android:id="@+id/detail_view_img"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_below="@+id/detail_imagen1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="5dp"
        app:srcCompat="@drawable/ic_logout" />


</RelativeLayout>
</ScrollView>